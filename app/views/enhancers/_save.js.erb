$("ul.errors").html("")
<% if @enhancer.errors.any? %>
  <% @enhancer.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".gene-index").html("<%= escape_javascript(render 'index') %>")
  $("#gene-modal").modal("hide")
<% end %>