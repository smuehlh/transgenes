<p id="notice"><%= notice %></p>

<h1>Listing Enhancers</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enhancers.each do |enhancer| %>
      <tr>
        <td><%= enhancer.data %></td>
        <td><%= link_to 'Show', enhancer %></td>
        <td><%= link_to 'Edit', edit_enhancer_path(enhancer) %></td>
        <td><%= link_to 'Destroy', enhancer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enhancer', new_enhancer_path %>
